{
  "borderNone": true,
  "brackets": true,
  "colons": false,
  "colors": false,
  "commaSpace": true,
  "commentSpace": true,
  "cssLiteral": false,
  "depthLimit": 4,
  "duplicates": true,
  "efficient": true,
  "emoji": false,
  "enforceBlockStyle": false,
  "enforceVarStyle": true,
  "extendPref": "@extend",
  "globalDupe": true,
  "indentSpaces": 2,
  "leadingZero": true,
  "maxWarnings": 10,
  "maxWarningsKill": false,
  "mixed": true,
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": true,
  "parenSpace": false,
  "placeholders": true,
  "quotePref":  "double",
  "semicolons": true,
  "sortOrder": "alphabetical",
  "stackedProperties": true,
  "universal": true,
  "valid": true,
  "whitespace": true,
  "zeroUnits": true,
  "zIndexDuplicates": false,
  "zIndexNormalize": false
}
