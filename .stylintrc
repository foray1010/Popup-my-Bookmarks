{
  "blocks": "never",
  "brackets": "never",
  "colons": "always",
  "colors": "always",
  "commaSpace": "always",
  "commentSpace": "always",
  "cssLiteral": "never",
  "depthLimit": 2,
  "duplicates": true,
  "efficient": "always",
  "extendPref": "@extend",
  "globalDupe": true,
  "indentPref": 2,
  "leadingZero": "never",
  "maxErrors": false,
  "maxWarnings": false,
  "mixed": true,
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": true,
  "none": "never",
  "parenSpace": "never",
  "placeholder": "always",
  "prefixVarsWithDollar": "always",
  "quotePref": "double",
  "stackedProperties": "never",
  "semicolons": "never",
  "sortOrder": "alphabetical",
  "universal": "never",
  "valid": true,
  "whitespace": true,
  "zeroUnits": "never",
  "zIndexNormalize": false
}
