doctype html
meta(charset="utf-8")
title Popup my Bookmarks
each x in ["common", "options"]
  link(rel="stylesheet", href="css/#{x}.css")

div#container
  div#logo-box
    div#logo-book
      div#star-five
    - for (var x = 1; x <= 4; x++)
      div.logo-memo(id="logo-memo#{x}")
    div#logo-title

  div#opt-box
  div#opt-button
    span#opt-msg-box
    button
    button

  div#footer
    hr.footer-hr
    div#donation
      div#donate-img
        form(action="https://www.paypal.com/cgi-bin/webscr", method="post", target="_blank")
          input(type="hidden", name="cmd", value="_s-xclick")
          input(type="hidden", name="encrypted", value="-----BEGIN PKCS7----------END PKCS7-----")
          input(type="image", name="submit", src="img/btn_donateCC_LG.png", alt="")
      p#donate-desc
        | If you like Popup my Bookmarks, please rate this extension on&nbsp;
        a.link(href="http://goo.gl/x9Wlq", target="_blank") Chrome Web Store
        |  and consider to buy me a drink via&nbsp;
        span#donate-here.link PayPal
        | !

each x in ["common", "options"]
  script(src="js/#{x}.js")
