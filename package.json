{
  "$schema": "https://json.schemastore.org/package",
  "name": "Popup-my-Bookmarks",
  "version": "7.1.1",
  "private": true,
  "description": "A more efficient way to view and manage your bookmarks menu.",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack",
    "ci": "npm-run-all -p 'lint:*' 'type:*' && yarn test && yarn build",
    "dev": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack",
    "eslint": "eslint --ext=cjs,cts,js,mjs,mts,ts,tsx --ignore-path=node_modules/@foray1010/eslint-config/eslintignore",
    "postinstall": "husky install",
    "lint:css": "yarn stylelint '**/*.css'",
    "lint:js": "yarn eslint .",
    "lint:md": "yarn remark .",
    "locales": "ts-node --transpile-only scripts/generateLocalesFromTransifex.ts",
    "md": "ts-node --transpile-only scripts/generateMarkdown.ts",
    "prettier": "prettier --ignore-path=node_modules/@foray1010/prettier-config/prettierignore",
    "remark": "remark --frail --ignore-path=node_modules/@foray1010/remark-preset/remarkignore --ignore-path-resolve-from=cwd",
    "stylelint": "stylelint --ignore-path=node_modules/@foray1010/stylelint-config/stylelintignore",
    "test": "cross-env NODE_ENV=test jest",
    "type:check": "tsc",
    "type:coverage": "type-coverage --strict --at-least 99 --detail --ignore-catch"
  },
  "browserslist": [
    "chrome >= 73"
  ],
  "dependencies": {
    "@babel/runtime": "7.16.7",
    "@fontsource/archivo-narrow": "4.5.4",
    "@rehooks/component-size": "1.0.3",
    "clsx": "1.1.1",
    "constate": "3.3.0",
    "lodash.debounce": "4.0.8",
    "nanoid": "3.2.0",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.27.0",
    "react-query": "3.34.15",
    "react-redux": "7.2.6",
    "react-virtual": "2.10.4",
    "redux": "4.1.2",
    "redux-saga": "1.1.3",
    "typesafe-actions": "5.1.0",
    "use-deep-compare": "1.1.0",
    "use-typed-event-listener": "3.0.0",
    "webextension-polyfill": "0.8.0"
  },
