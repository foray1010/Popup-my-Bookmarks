    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.28.1",
    "react-query": "3.34.16",
    "react-redux": "7.2.6",
    "react-virtual": "2.10.4",
    "redux": "4.1.2",
    "redux-saga": "1.1.3",
    "typesafe-actions": "5.1.0",
    "use-deep-compare": "1.1.0",
    "use-typed-event-listener": "3.0.0",
    "webextension-polyfill": "0.8.0"
  },
