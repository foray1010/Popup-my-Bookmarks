{
  "name": "Popup-my-Bookmarks",
  "version": "6.1.0",
  "engines": {
    "node": ">=8.9"
  },
  "browserslist": [
    "chrome >= 55"
  ],
  "description": "A more efficient way to view and manage your bookmarks menu.",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "ci": "npm-run-all -p lint:* test type:* && yarn build",
    "dev": "cross-env NODE_ENV=development webpack",
    "lint:css": "stylelint 'src/**/*.{css,ts,tsx}'",
    "lint:js": "eslint --ext ts,tsx .",
    "lint:md": "markdownlint *.md",
    "locales": "ts-node --transpile-only scripts/generateLocalesFromTransifex.ts",
    "md": "ts-node --transpile-only scripts/generateMarkdown.ts",
    "test": "cross-env NODE_ENV=test jest --config .jestrc.json",
    "type:check": "tsc",
    "type:coverage": "type-coverage --strict --at-least 99.9 --detail --ignore-catch"
  },
