    "@foray1010/eslint-config": "4.5.1",
    "@foray1010/prettier-config": "4.2.0",
    "@foray1010/remark-preset": "3.4.1",
    "@foray1010/stylelint-config": "2.3.2",
    "@foray1010/tsconfig": "4.1.1",
    "@svgr/webpack": "5.5.0",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.0.3",
