    "@types/zip-webpack-plugin": "3.0.3",
    "axios": "0.27.2",
    "chrome-manifest-loader": "0.3.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
