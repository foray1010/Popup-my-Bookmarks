{
  "$schema": "https://json.schemastore.org/package",
  "name": "Popup-my-Bookmarks",
  "version": "7.1.1",
  "private": true,
  "description": "A more efficient way to view and manage your bookmarks menu.",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn webpack",
    "ci": "npm-run-all -p 'lint:*' 'type:*' && yarn test && yarn build",
    "dev": "cross-env NODE_ENV=development yarn webpack",
    "eslint": "eslint --ext=cjs,cts,js,mjs,mts,ts,tsx --ignore-path=node_modules/@foray1010/eslint-config/eslintignore",
    "postinstall": "husky install",
    "lint:css": "yarn stylelint '**/*.css'",
    "lint:js": "yarn eslint .",
    "lint:md": "yarn remark .",
    "locales": "node -r @swc-node/register scripts/generateLocalesFromTransifex.ts",
    "md": "node -r @swc-node/register scripts/generateMarkdown.ts",
    "prettier": "prettier --ignore-path=node_modules/@foray1010/prettier-config/prettierignore",
    "remark": "remark --frail --ignore-path=node_modules/@foray1010/remark-preset/remarkignore --ignore-path-resolve-from=cwd",
    "stylelint": "stylelint --ignore-path=node_modules/@foray1010/stylelint-config/stylelintignore",
    "test": "cross-env NODE_ENV=test jest",
    "type:check": "tsc",
    "type:coverage": "type-coverage --strict --at-least 99 --detail --ignore-catch",
    "webpack": "node -r @swc-node/register node_modules/.bin/webpack"
  },
  "browserslist": [
    "chrome >= 88"
  ],
  "dependencies": {
    "@fontsource/archivo-narrow": "4.5.9",
    "@reach/portal": "0.17.0",
    "@tanstack/react-query": "4.2.3",
    "@tanstack/react-query-devtools": "4.0.10",
    "clsx": "1.2.1",
    "constate": "3.3.2",
    "core-js": "3.25.0",
    "immer": "9.0.15",
    "lodash.debounce": "4.0.8",
    "nanoid": "4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-virtual": "2.10.4",
    "use-deep-compare": "1.1.0",
    "use-resize-observer": "9.0.2",
    "use-typed-event-listener": "3.0.0",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@foray1010/eslint-config": "7.3.4",
    "@foray1010/jest-preset": "2.0.0",
    "@foray1010/prettier-config": "7.0.3",
    "@foray1010/remark-preset": "7.0.2",
    "@foray1010/stylelint-config": "7.0.1",
    "@foray1010/tsconfig": "8.0.2",
    "@parcel/css": "1.13.0",
    "@svgr/webpack": "6.3.1",
    "@swc-node/register": "1.5.1",
    "@swc/core": "1.2.224",
    "@swc/jest": "0.2.22",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@transifex/api": "4.2.1",
    "@types/duplicate-package-checker-webpack-plugin": "2.1.2",
    "@types/firefox-webext-browser": "94.0.1",
    "@types/jest": "28.1.8",
    "@types/lodash.debounce": "4.0.7",
