    "typesafe-actions": "5.1.0",
    "use-typed-event-listener": "2.0.0",
    "webextension-polyfill": "0.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@commitlint/cli": "8.3.5",
