    "@types/bluebird": "3.5.33",
    "@types/copy-webpack-plugin": "6.4.0",
    "@types/css-minimizer-webpack-plugin": "1.1.1",
    "@types/duplicate-package-checker-webpack-plugin": "2.1.0",
