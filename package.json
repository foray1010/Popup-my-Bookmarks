{
  "$schema": "https://json.schemastore.org/package",
  "name": "Popup-my-Bookmarks",
  "version": "7.1.1",
  "private": true,
  "description": "A more efficient way to view and manage your bookmarks menu.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production yarn webpack",
    "ci": "npm-run-all -p 'lint:*' 'type:*' && yarn test && yarn build && size-limit",
    "dev": "cross-env NODE_ENV=development yarn webpack",
    "eslint": "eslint --ext=cjs,cts,js,mjs,mts,ts,tsx --ignore-path=node_modules/@foray1010/eslint-config/eslintignore",
    "postinstall": "husky install",
    "lint:css": "yarn stylelint '**/*.css'",
    "lint:js": "yarn eslint .",
    "lint:md": "yarn remark .",
    "locales": "ts-node scripts/generateLocalesFromTransifex.ts",
    "md": "ts-node scripts/generateMarkdown.ts",
    "prettier": "prettier --ignore-path=node_modules/@foray1010/prettier-config/prettierignore",
    "remark": "remark --frail --ignore-path=node_modules/@foray1010/remark-preset/remarkignore --ignore-path-resolve-from=cwd --silently-ignore",
    "stylelint": "stylelint --ignore-path=node_modules/@foray1010/stylelint-config/stylelintignore",
    "test": "cross-env NODE_ENV=test jest",
    "type:check": "tsc",
    "type:coverage": "type-coverage --strict --at-least 99 --detail --ignore-catch",
    "webpack": "ts-node node_modules/.bin/webpack-cli"
  },
  "dependencies": {
    "@fontsource/archivo-narrow": "4.5.12",
    "@tanstack/react-query": "4.26.1",
    "@tanstack/react-query-devtools": "4.26.1",
    "clsx": "1.2.1",
    "constate": "3.3.2",
    "core-js": "3.29.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.5",
    "react-virtual": "2.10.4",
    "use-debounce": "9.0.3",
    "use-deep-compare": "1.1.0",
    "use-resize-observer": "9.1.0",
    "use-typed-event-listener": "4.0.2",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@foray1010/eslint-config": "9.2.0",
    "@foray1010/jest-preset": "3.0.0",
    "@foray1010/prettier-config": "8.0.1",
    "@foray1010/remark-preset": "8.0.0",
    "@foray1010/stylelint-config": "10.1.1",
    "@foray1010/tsconfig": "10.0.0",
    "@size-limit/file": "8.2.4",
    "@svgr/webpack": "6.5.1",
    "@swc/core": "1.3.34",
    "@swc/jest": "0.2.24",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@transifex/api": "5.1.1",
    "@types/duplicate-package-checker-webpack-plugin": "2.1.2",
    "@types/firefox-webext-browser": "111.0.0",
    "@types/jest": "29.4.2",
    "@types/node": "16.18.16",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/script-ext-html-webpack-plugin": "2.1.3",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/zip-webpack-plugin": "3.0.3",
    "axios": "1.3.4",
    "chrome-manifest-loader": "0.3.0",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "eslint": "8.36.0",
    "extract-loader": "5.1.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "ignore-sync": "6.0.2",
    "image-process-loader": "1.1.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lightningcss": "1.19.0",
    "lightningcss-loader": "2.1.0",
    "lint-staged": "13.1.2",
    "mini-css-extract-plugin": "2.7.3",
    "node-notifier": "10.0.1",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-loader": "7.0.2",
    "postcss-normalize": "10.0.1",
    "prettier": "2.8.4",
    "remark-cli": "11.0.0",
    "remeda": "1.9.0",
    "script-ext-html-webpack-plugin": "2.1.5",
    "sharp": "0.31.3",
    "size-limit": "8.2.4",
    "style-loader": "3.3.2",
    "stylelint": "15.2.0",
    "swc-loader": "0.2.3",
    "terser-webpack-plugin": "5.3.7",
    "ts-node": "10.9.1",
    "type-coverage": "2.24.1",
    "typescript": "4.9.5",
    "webpack": "5.76.1",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.0.1",
    "yaml-loader": "0.8.0",
    "zip-webpack-plugin": "4.0.1"
  },
  "engines": {
    "node": "^16.13.0 || >=18.12.0"
  }
}
